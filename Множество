import java.util.HashSet;

// Класс, представляющий структуру данных "Множество"
public class Множество {
    
    private HashSet<Object> set; // Хранилище элементов множества
    
    // Конструктор класса
    public Множество() {
        set = new HashSet<>();
    }
    
    // Метод для добавления элемента в множество
    public void добавитьЭлемент(Object элемент) {
        set.add(элемент);
    }
    
    // Метод для удаления элемента из множества
    public void удалитьЭлемент(Object элемент) {
        set.remove(элемент);
    }
    
    // Метод для проверки наличия элемента в множестве
    public boolean содержитЭлемент(Object элемент) {
        return set.contains(элемент);
    }
    
    // Метод для получения размера множества
    public int размер() {
        return set.size();
    }
    
    // Метод для очистки множества
    public void очистить() {
        set.clear();
    }
}
